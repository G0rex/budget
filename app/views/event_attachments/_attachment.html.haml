:css
  .document {
    margin: 1%;
    padding: 10px 5px 10px 5px;
    background-color: #e7f4f9;
    width: 48%;
  }
-#=binding.pry
.document.pull-left{'data-id' => "#{event_attachment.id}"}
  .col-sm-1
    - if event_attachment.doc_file?
      = link_to event_attachment.doc_file.url do
        %i.fa{ class: "#{get_file_type_icon(event_attachment.doc_file_identifier)}" }
  .col-sm-10
    %a{ href: '#', id: 'name', 'data-type' => 'text', 'data-pk' => "#{event_attachment.id}", 'data-resource' => 'event_attachment', 'data-url' => calendar_event_event_attachment_path(event_attachment.event.calendar.id, event_attachment.event.id, event_attachment.id), 'data-emptytext' => 'Вкажіть назву документа...' }
      = event_attachment.name
  .col-sm-1
    -#= link_to event_attachment, :remote => true, :method => :delete, :data => { :confirm => 'Are you sure?' }, :class => '' do
    -#  %i.fa.fa-times.pull-right

:javascript

  $('.document a[data-pk="#{event_attachment.id}"]').editable();
