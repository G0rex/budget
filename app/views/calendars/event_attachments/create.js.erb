<% if @event_attachment.errors.empty? %>
    $('.attachment_files table').prepend('<%= escape_javascript( render :partial => 'attachment', :locals => { :event_attachment => @event_attachment } ) %>')
    $('.attachment_files a[data-pk]').editable();
<% else %>
    alert('Помилка завантаження файлу. Перевірте дані'+@event_attachment.errors)
<% end %>
