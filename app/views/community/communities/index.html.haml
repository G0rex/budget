:css
  .map_wrap {
    height: 700px;
  }
= content_for :header do
  .head.pull-left
    = t('community.community.index.header')

.map_wrap
  = render partial: 'map_ukraine'

.col-sm-12{:style => 'margin-top: 10px;'}
  = link_to 'Редагувати', '/community/communities_edit', :class => 'btn btn-primary', :id => 'edit_btn'
  = link_to 'Завантажити', '/community/communities_load', :class => 'btn btn-primary'
  #embed_code_timeline.pull-right{:style => 'display: none;'}
    = render partial: 'widgets/visify/embed_code'
