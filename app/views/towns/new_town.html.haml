= content_for :header do
  .head.pull-left
    =t('.create_city')

=bootstrap_form_for(@town) do |f|
  =f.number_field :koatuu
  =f.text_field :title
  =f.select :level,Town.get_levels_array.map{|level|[level.last,level.first]},{label: t('.level')}
  =f.submit t('.create_city'), {class: 'btn btn-primary'}



