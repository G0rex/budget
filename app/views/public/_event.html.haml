.circle.pull-left
  = image_tag("calendar/timeline-city.png", width: '50px')

%ul.tabs.pull-left{role: 'tablist'}
  %li.active
    %a{href: '#calendar_news', role: 'tab', 'data-toggle' => 'tab'}
      .news
  %li
    %a{href: '#calendar_event', role: 'tab', 'data-toggle' => 'tab'}
      .event

.tab-content.col-sm-12
  #calendar_news.tab-pane.fade.in.active
  #calendar_event.tab-pane.fade
