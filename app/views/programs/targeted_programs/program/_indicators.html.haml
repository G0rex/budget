    .col-xs-6
      .tab-content.indicate-pane
        - indicators_group.each_with_index do |indicator, i|
        - indicator[indicators_array_index].each_with_index do |item, j|
          -#.tab-pane.fade{id: 'expense', role: 'tabpanel'}
          -#- tab_id = "tab-#{i}-#{j}"
          - tab_id_content = "tab-#{i}-#{j}-content"
            .tab-pane.fade{id: tab_id_content, role: 'tabpanel'}
              = render partial: 'programs/targeted_programs/program/indicator', locals: { indicator: item }
