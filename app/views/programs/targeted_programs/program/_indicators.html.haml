            .tab-pane.fade{id: "tab-#{indicator_group[indicators_title_index]}"}
              %ul.nav.nav-pills
                - indicator_group[indicators_array_index].each_with_index do |indicator, j|
                  - tab_id = "tab-#{i}-#{j}"
                  - tab_id_content = "#tab-#{i}-#{j}-content"
                  - active = (i == 0 and j == 0 ? 'active in' : '')
                  %li.indicator-title
