:scss
  .program_indicators {
    height: 300px;
    .main-nav-panel{
      padding-bottom: 25px;
      width: 20%;
    }
    .indicate-pane{
      margin-top: 130px;
    }
    li {
      //text-align: center;
    }

    .tab-content {
      padding: 15px 10px;
    }
    .indicator-dropdown-title{
      width: 400px;


      .indicator-title{
        overflow: hidden;
        text-overflow: ellipsis;
        font-size: 16px;
      }
    }
    .indicate-list{
      height: 300px;
      overflow: auto;
      .indicator-title{
        width: 100%;
      }
    }

  }
